.wx-switch-input{
    width:88rpx !important;
    height:48rpx !important;
    background: #4fd866 !important;
    border: #4fd866 !important; 
}
/*白色样式（false的样式）*/
.wx-switch-input::before{
    width:88rpx !important;
    height: 48rpx !important;    
}
/*绿色样式（true的样式）*/
.wx-switch-input::after{
    width: 40rpx !important;
    height: 40rpx !important;
    margin-top: 4rpx;
}

